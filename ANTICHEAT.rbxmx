local hum = script.Parent:WaitForChild("Humanoid")
local localplayer = game.Players.LocalPlayer
local character = localplayer.Character
local HRP = character:WaitForChild("HumanoidRootPart")

local message = "LAPRIS anticheat systems has detected some suspicous activity during this session for that reason, your session has been terminated."
print("Lapris Systems Loaded.")
--Humanoid player hacks
while true do
	wait(0.0001) 
	if hum.WalkSpeed ~= 16 then

		localplayer:Kick(message)
	end
	if hum.JumpPower ~= 50 then
		localplayer:Kick(message)

	end


	if hum.MaxHealth > 100 then
		localplayer:Kick(message)
	end

	if hum.Health > 100 then
		localplayer:Kick(message)
	end


end

--Fly hacks
HRP.ChildAdded:Connect(function(Object)
	if Object.ClassName == "BodyGyro" or Object.ClassName == "BodyPosition" or Object.ClassName == "BodyVelocity" then
		Object:Destroy()
		localplayer:Kick(message)
	end
end)

